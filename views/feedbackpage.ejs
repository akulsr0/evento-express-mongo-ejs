<html>
  <head>
    <title>Evento: Deleted</title>
    <link rel="stylesheet" href="/css/styles.css" />
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
      integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <header>
      <center>
        <h1 class="titleText">Evento</h1>
        <div class="menuDiv">
          <a href="/" class="menuItem">Home</a>
          <a href="/events" class="menuItem">Events</a>
          <a href="/about-us" class="menuItem">About Us</a>
          <a href="/contact-us" class="menuItem">Contact Us</a>
          <hr size="1" width="60%" style="margin-top: 10px;" />
        </div>
      </center>
    </header>
    <center>
      <div style="margin-top: 10px;"> 
      <div class="upEventsTitleDiv">
        <h3 class="opensans" style="font-weight: 700;">Feedbacks</h3>
      </div>
    </center>
    <div style="margin: 10px;">
      <% feedbackResult.map(feedback => { %>
          <div class="feedback">
            <strong><%=feedback.name%></strong><br>
            <small><%=feedback.email%></small>
            <p><%=feedback.feedback%></p>
          </div>
      <% }) %>
    </div>
  </body>
</html>
